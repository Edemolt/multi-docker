
{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "clinet",
            "image": "demolt/muti-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "demolt/muti-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "demolt/muti-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "demolt/muti-nginx", 
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["clinet", "server"]
        }
    ]
}